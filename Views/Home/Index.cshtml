@model IEnumerable<GestionnaireUtilisateurs.Models.AspNetUsers>


@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}




<div class="dashboard-main-wrapper" style="font-family: 'Catamaran', sans-serif; font-size:15px;background-color:#e9e9e9">

    <div class="dashboard-wrapper">
        <div class="dashboard-finance">
            <div class="container-fluid dashboard-content">

                <div class="tab-content" style="text-align:left; background-color: white; width: 100%; padding-top:15px; box-shadow: 0 17px 41px -21px black;border-top:5px solid #09aacc">
                    <div class="card-body">
                        <div class="card-body border-top">

                            <a href="@Url.Action("AddUser", "Home")" class="btn btn" style="background-color:#09aacc ; color:white;">
                                <i class="fa fa-plus"> Ajouter  </i>
                            </a>
                            <button type="button" class="btn btn" style="background-color:#09aacc ; color:white;">
                                <i class="fa fa-edit"> Modifier  </i>
                            </button>
                            <button type="button" class="btn btn" style="background-color:#09aacc ; color:white;">
                                <i class="fa fa-trash"> Supprimer  </i>
                            </button>
                            <a href="@Url.Action("UserTache", "Home")" class="btn btn" style="background-color:#09aacc ; color:white; margin-left: 63%;">
                                <i class="fa fa-sliders-h"> Gestion des roles  </i>
                            </a>
                        </div>
                        <table class="table">
                            <thead>
                                <tr>

                                    <th>
                                        @Html.DisplayNameFor(model => model.Nom)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.Prenom)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.Email)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.Ville)
                                    </th>
                                    <th></th>
                                </tr>

                            </thead>
                            <tbody>
                                @foreach (var item in Model) {
                                    <tr>
                                        <td>
                                            @Html.DisplayFor(modelItem=> item.Nom)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem=> item.Prenom)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem=> item.Email)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem=> item.Ville)
                                        </td>
                                    </tr>
                                }
                            </tbody>


                        </table>

                    </div>
                </div>

            </div>
        </div>
    </div>
</div>






